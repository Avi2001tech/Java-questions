import java.util.*;
public class Main
{
    public static int count_digit(int x){
        int count =0;
        while(x!= 0){
            //System.out.println(x);
            count++;
            x=x/10;
        }
        return count;
    }
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int num = sc.nextInt();
	    int result =count_digit(num);
		System.out.println(result);
	}
}
