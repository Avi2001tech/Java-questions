import java.util.*;
public class Main
{
    public static int reverse_num(int x){
        int r_n = 0;
        while(x!= 0){
            r_n = (x%10) + r_n*10;
            x=x/10;
        }
        return r_n;
    }
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int num = sc.nextInt();
	    int result =reverse_num(num);
		System.out.println(result);
	}
}
